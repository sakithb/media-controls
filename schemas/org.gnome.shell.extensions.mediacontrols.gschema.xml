<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.mediacontrols" path="/org/gnome/shell/extensions/mediacontrols/">
    <key name="max-widget-width" type="i">
      <default>200</default>
    </key>
    <key name="update-delay" type="i">
      <default>100</default>
    </key>
    <key name="show-text" type="b">
      <default>true</default>
    </key>
    <key name="show-player-icon" type="b">
      <default>true</default>
    </key>
    <key name="show-control-icons" type="b">
      <default>true</default>
    </key>
    <key name="show-seperators" type="b">
      <default>true</default>
    </key>
    <key name="show-playpause-icon" type="b">
      <default>true</default>
    </key>
    <key name="show-prev-icon" type="b">
      <default>true</default>
    </key>
    <key name="show-next-icon" type="b">
      <default>true</default>
    </key>
    <key name="show-sources-menu" type="b">
      <default>true</default>
    </key>
    <key name="colored-player-icon" type="b">
      <default>false</default>
    </key>
    <key name="seperator-chars" type="as">
      <default>['|', '|']</default>
    </key>
    <key name="mouse-actions" type="as">
      <default>['toggle_play', 'toggle_menu', 'none', 'none', 'none', 'none', 'none', 'none']</default>
    </key>
    <key name="extension-position" type="s">
      <default>'center'</default>
    </key>
    <key name="extension-index" type="i">
      <default>0</default>
    </key>
    <key name="element-order" type="as">
      <default>['icon', 'title', 'controls', 'menu']</default>
    </key>
    <key name="track-label" type="as">
      <default>['track', '-', 'artist']</default>
    </key>
    <key name="cache-images" type="b">
      <default>true</default>
    </key>
  </schema>
</schemalist>
